{
  "_id": "bitbucket_clone",
  "startUrl": ["https://bitbucket.org/dashboard/repositories"],
  "selectors": [
    {
      "id": "repo_card",
      "type": "SelectorElement",
      "parentSelectors": ["_root", "page"],
      "selector": "tr.css-odi3kb",
      "multiple": true,
      "delay": 0
    },
    {
      "id": "page",
      "type": "SelectorLink",
      "parentSelectors": ["_root", "page"],
      "selector": "a[page]",
      "multiple": true,
      "delay": 0
    },
    {
      "id": "repo_name",
      "type": "SelectorLink",
      "parentSelectors": ["repo_card"],
      "selector": "td:first > div > div:nth-of-type(2) > span > a",
      "multiple": false,
      "delay": 0
    },
    {
      "id": "clone_btn",
      "type": "SelectorElementClick",
      "parentSelectors": ["repo_name"],
      "selector": "section[role='dialog']",
      "multiple": false,
      "delay": 2000,
      "clickElementSelector": "button span:contains('Clone')",
      "clickType": "clickOnce",
      "discardInitialElements": "do-not-discard",
      "clickElementUniquenessType": "uniqueText"
    },
    {
      "id": "clone",
      "type": "SelectorElementAttribute",
      "parentSelectors": ["clone_btn"],
      "selector": "input[aria-label=\"Copyable input\"]",
      "multiple": false,
      "extractAttribute": "value",
      "delay": 0
    }
  ]
}
